[req]
default_bits = 2048
prompt = no
distinguished_name = dn
req_extensions = req_ext
x509_extensions = v3_ca

[dn]
C = VN
ST = HCM
L = Thu Duc
O = PROJECT_OGANI
OU = Dev
CN = localhost

[req_ext]
subjectAltName = @alt_names

[v3_ca]
subjectAltName = @alt_names
basicConstraints = CA:TRUE

[alt_names]
DNS.1 = localhost
IP.1 = 127.0.0.1
